<h1 >Edit your kiosk info</h1>
<ul>
  <%= form_for :kiosk, url: admin_kiosk_path(@kiosk), method: 'put'  do |f| %>
  <li>
    <%= f.label :longitude %>
    <%= f.text_field :longitude, :value => @kiosk.longitude%>
  </li>
  <li>
    <%= f.label :latitude%>
    <%= f.text_field :latitude, :value => @kiosk.latitude %>
  </li>
   <li>
    <%= f.label :location_id %>
    <%= f.text_field :location_id, :value => @kiosk.location_id %>
  </li>
  <li>
    <%= f.label :provider_id %>
    <%= f.text_field :provider_id, :value => @kiosk.provider_id  %>
  </li>
  <li>
    <%= f.submit %>
  </li>
<% end%>
</ul>
